
@{
    ViewData["Title"] = "Edit";
}
@model DartLeague.Web.Areas.Site.Models.PagePartViewModel
<h2>Create Dart Event</h2>
<form asp-area="Site" asp-controller="PagePart" asp-action="Edit" asp-route-id="@Model.Id" method="post">
    @await Html.PartialAsync("_pagePartFields")
    <div>
        <button type="submit" class="btn btn-default"><i class="fa fa-floppy-o fa-lg"></i> Update</button>
        <a class="btn btn-danger" asp-area="Site" asp-controller="PagePart" asp-action="Index">Cancel</a>
    </div>
</form>


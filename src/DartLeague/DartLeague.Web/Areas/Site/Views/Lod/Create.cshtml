
@{
    ViewData["Title"] = "Add Luck of the Draw";
}
@Html.ValidationSummary()
<div class="container">
    <h2>Create Dart Event</h2>
    <form asp-controller="Lod" enctype="multipart/form-data" asp-action="Create" method="post">
        @Html.Partial("_lodFields")
        <div>
            <button type="submit" class="btn btn-default"><i class="fa fa-floppy-o fa-lg"></i> Add</button>
            <a class="btn btn-danger" asp-controller="Lod" asp-action="Index">Cancel</a>
        </div>
    </form>
</div>
<style>
    validation-summary-errors {
        color: crimson;
    }
</style>

